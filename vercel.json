{
    "version": 2,
    "functions": {
      "api/*.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      { "src": "/api/.*", "dest": "/api/routes.ts" },
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }